{
  "name": "logicmoon",
  "version": "3.2.0",
  "private": true,
  "title": "Logic Moon Music Home",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "browser-sync start --proxy 'http://localhost:8080' -f  *",
    "rundeploy": "node deploy.js",
    "deploy": "npm-run-all build rundeploy"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "animejs": "^3.2.1",
    "axios": "^0.27.2",
    "bootstrap": "^5.2.0",
    "core-js": "^3.8.3",
    "popper.js": "^1.16.1",
    "scrollreveal": "^4.0.9",
    "shufflejs": "^5.4.1",
    "vue": "^3.2.13"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@squoosh/lib": "^0.4.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "ftp-deploy": "^2.4.3",
    "image-minimizer-webpack-plugin": "^3.3.0",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^10.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "sass": "^1.45.1",
    "sass-loader": "^13.0.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "no-unused-vars": "warn"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
